{
  "name": "todayrates",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "db:setup": "node scripts/migrate.js && node scripts/seed.js",
    "predeploy": "npm run build:production && cp public/404.html dist/404.html",
    "predeploy:staging": "npm run build:staging && cp public/404.html dist/404.html",
    "deploy": "gh-pages -d dist",
    "deploy:staging": "npm run predeploy:staging && gh-pages -d dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.91.0",
    "react": "^18.3.1",
    "react-datepicker": "^9.1.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.12.0",
    "react-select": "^5.10.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.23",
    "dotenv": "^17.2.3",
    "gh-pages": "^6.3.0",
    "pg": "^8.17.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^5.4.2"
  }
}
